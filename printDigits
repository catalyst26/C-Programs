#include <iostream>

int main()
{
    
    using std::cout;
    using std::cin;
    using std::endl;
    
    int n, r;
    cout << "Enter number to print individual digits: ";
    cin >> n;
    while (n > 0)
    {
        r = n % 10;
        n /= 10;
        cout << r << endl;
    }
    
    return 0;
}
