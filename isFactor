#include <iostream>

int main() {
    
    using std::cout;
    using std::cin;
    using std::endl;
    
    int n, i;
    cout<<"Enter number you want to find factors for: ";
    cin>>n;
    for (i = 1; i <= n; i++)
    {
        if (n % i == 0)
            cout << i << " is a factor of " << n << endl;
    }
    
    
    
    return 0;
}
